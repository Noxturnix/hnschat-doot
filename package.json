  },
  "homepage": "https://github.com/Noxturnix/hnschat-doot#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "dotenv": "^10.0.0",
    "rimraf": "^3.0.2",
    "sed-lite": "^0.8.4",
    "tunnel": "^0.0.6",
    "unescape": "^1.0.1",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/node": "^16.7.4",
    "@types/tunnel": "^0.0.3",
    "@types/websocket": "^1.0.5",
